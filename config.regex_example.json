{
  "ndi": {
    "source_pattern": ".*_led",
    "pattern_type": "regex",
    "enable_plural_handling": false,
    "case_sensitive": false,
    "scan_timeout": 2,
    "color_format": "bgra",
    "auto_switch": true
  },
  "display": {
    "resolution": [800, 800],
    "fullscreen": true,
    "video_driver": "auto",
    "show_fps": false
  },
  "content": {
    "resolution": [320, 320],
    "position": [0, 0],
    "rotation": 180,
    "scaling": "none"
  },
  "performance": {
    "cpu_governor": "performance",
    "process_priority": -10
  },
  "logging": {
    "level": "INFO",
    "show_fps_interval": 10
  }
}
